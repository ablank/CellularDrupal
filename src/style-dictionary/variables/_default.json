{
  "path": {
    "assets": "../assets",
    "images": "{path.assets}/images",
    "icons": "{path.assets}/icons"
  },
  "default": {
    "unit": "em",
    "color": { "value": "{swatch.black}" },
    "speed": { "value": "500ms", "category": "metric" },
    "easing": { "value": "ease" },
    "font": {
      "family": { "value": "{font.times.value}" },
      "size": { "value": "1{default.unit}" }
    },
    "border": { "value": "1px solid" },
    "radius": { "value": "{xs.value}{default.unit}" },
    "offset": { "value": "{baseline.value}{default.unit}" },
    "transition": { "value": "all {default.speed.value} ease 0" }
  },
  "xxs": { "value": "0.2" },
  "xs": { "value": "0.4" },
  "small": { "value": "0.75" },
  "baseline": { "value": "1" },
  "large": { "value": "1.4" },
  "xl": { "value": "2.5" },
  "xxl": { "value": "5" },
  "max-width": { "value": "72em" },
  "offset": {
    "x": { "value": "0{default.unit}" },
    "y": { "value": "1{default.unit}" }
  },
  "tranparency": { "value": "{xs.value}" },
  "adjust": { "value": "15%" },
  "transition": { "value": "all {default.speed.value}" },
  "boxshadow": {
    "value": "inset {xs.value}{default.unit} {xs.value}{default.unit} {xs.value}{default.unit} 1px {color.neutral.value};",
    "category": "default"
  },
  "dropshadow": {
    "value": "{offset.x.value} {offset.y.value} {xs.value}{default.unit} {color.shadow.value};",
    "category": "default"
  }
}
